{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "iam:ListUsers",
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "iam:ListSSHPublicKeys",
                "iam:GetSSHPublicKey"
            ],
            "Resource": [
                "arn:aws:iam::323270637157:user/*"
            ]
        },
        {
            "Action": "iam:GetGroup",
            "Resource": [
                "arn:aws:iam::323270637157:group/Administrators"
            ],
            "Effect": "Allow"
        },
        {
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::intellipoint-wordpress-backups"
            ],
            "Effect": "Allow",
            "Sid": "ListObjectsInBucket"
        },
        {
            "Action": "s3:*Object",
            "Resource": [
                "arn:aws:s3:::intellipoint-wordpress-backups/*"
            ],
            "Effect": "Allow",
            "Sid": "AllObjectActions"
        },
        {
            "Action": [
                "ec2:DescribeInstances",
                "ec2:DescribeTags",
                "ec2:CreateTags"
            ],
            "Effect": "Allow",
            "Resource": "*"
        }
    ]
}